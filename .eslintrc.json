{
  "root": true,
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "off",
    "semi": "off",
    "node/no-missing-import": "error"
  }
}
